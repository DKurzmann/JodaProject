{
  "name": "jodaproject",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/react": "^3.0.9",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "astro": "^4.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-use": "^17.4.2",
    "starfield-react": "^3.1.0-dev-5"
  },
  "devDependencies": {
    "prettier": "^3.2.2",
    "prettier-plugin-astro": "^0.12.3"
  },
  "prettier": {
    "singleQuote": true,
    "bracketSameLine": true,
    "semi": false,
    "plugins": [
      "prettier-plugin-astro"
    ],
    "overrides": [
      {
        "files": "*.astro",
        "options": {
          "parser": "astro"
        }
      }
    ]
  }
}
